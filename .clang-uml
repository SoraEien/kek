compilation_database_dir: debug
output_directory: docs/diagrams
diagrams:
  main_package:
    type: package
    glob:
      - src/**/*.h
      - src/**/*.cc
    include:
      namespaces:
        - clanguml
    using_namespace:
      - clanguml
    plantuml:
      before:
        - 'title clang-uml namespaces'